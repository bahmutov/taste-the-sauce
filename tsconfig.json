{
  "compilerOptions": {
    "moduleResolution": "node",
    "types": ["cypress", "cypress-map"],
    "target": "es2016",
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "cypress/support/*.ts",
    "cypress/e2e/**/*.js",
    "cypress/e2e/**/*.ts",
    "cypress/support/pages/checkout.page.ts"
  ]
}
