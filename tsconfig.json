{
  "compilerOptions": {
    "types": ["cypress", "cypress-map", "node"],
    "target": "es2016",
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node"
  },
  "include": ["cypress/e2e/*.ts", "cypress/support/*.ts"]
}
